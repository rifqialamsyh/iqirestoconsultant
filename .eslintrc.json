{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true,
    "jasmine": true
  },
  "extends": ["airbnb-base"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["jasmine"],
  "rules": {
    "no-console": 0,
    "no-underscore-dangle": 0,
    "object-curly-newline": 0,
    "operator-linebreak": ["error", "after"],
    "linebreak-style": 0,
    "import/no-extraneous-dependencies": 0,
    "no-restricted-globals": 0,
    "import/prefer-default-export": 0,
    "no-multiple-empty-lines": 0,
    "consistent-return": 0,
    "no-prototype-builtins": 0,
    "no-return-assign": 0,
    "class-methods-use-this": 0,
    "no-new": 0,
    "no-plusplus": 0,
    "no-await-in-loop": 0,
    "no-undef": 0,
    "no-unused-vars": 0,
    "func-names": 0
  }
}
